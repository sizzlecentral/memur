<h1>Sign Up</h1>
<%= link_to("Back to Memes", "/memes") %>

<%= form_for(@user) do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br>
    <%= f.text_field :password_confirmation %>
  </p>
  <p>
    <%= f.submit "Create New User" %>
  </p>
<% end %>
